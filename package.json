    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleNameMapper": {
      "\\.(png|svg|md|css)$": "<rootDir>/test/helpers/fileMock.js"
    },
    "setupFiles": [
